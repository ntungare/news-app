{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "ui-server",
    "version": "1.0.0",
    "description": "ui-server for the app",
    "main": "./dist/server/index.js",
    "keywords": [
        "express",
        "jwt",
        "react",
        "ssr"
    ],
    "author": {
        "name": "Ninad Tungare",
        "email": "ninad.tungare@gmail.com"
    },
    "license": "MIT",
    "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48",
    "scripts": {
        "check": "tsc --noEmit --extendedDiagnostics",
        "clean": "rm -rf dist",
        "build": "NODE_ENV=production pnpm run build:delegate",
        "build:delegate": "pnpm run build:node && pnpm run build:bundles",
        "build:node": "tsc",
        "build:bundles": "pnpm run build:bundle:client --mode=production && pnpm run build:bundle:server --mode=production",
        "build:bundle:client": "vite build --config ./vite.client.config.ts",
        "build:bundle:server": "vite build --config ./vite.server.config.ts",
        "start": "node ./dist/server/index.js",
        "start:dev": "NODE_ENV=development concurrently --prefixColors auto pnpm:start:dev:node pnpm:start:dev:bundle:client pnpm:start:dev:bundle:server",
        "start:dev:node": "ts-node-dev --inspect=0.0.0.0:9220 -- ./src/server/index.ts",
        "start:dev:bundles": "concurrently --prefixColors auto pnpm:start:dev:bundle:client pnpm:start:dev:bundle:server",
        "start:dev:bundle:client": "pnpm run build:bundle:client --minify=false --watch --mode=development",
        "start:dev:bundle:server": "pnpm run build:bundle:server --minify=false --watch --mode=development",
        "lint": "oxlint --type-aware src",
        "format": "oxfmt src",
        "fix-all": "pnpm run lint --fix && pnpm run format",
        "update": "sh ./update-all-dependencies.sh"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.90.16",
        "@tanstack/react-query-devtools": "^5.91.2",
        "axios": "^1.13.2",
        "body-parser": "^2.2.2",
        "classnames": "^2.5.1",
        "compression": "^1.8.1",
        "country-flag-icons": "^1.6.4",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "htmlescape": "^1.1.1",
        "lodash-es": "^4.17.22",
        "lru-cache": "^11.2.4",
        "morgan": "^1.10.1",
        "object-hash": "^3.0.0",
        "qs": "^6.14.1",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-icons": "^5.5.0",
        "redis": "^5.10.0",
        "tailwindcss": "^4.1.18",
        "uuid": "^13.0.0",
        "web-vitals": "^5.1.0"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.18",
        "@tanstack/eslint-plugin-query": "^5.91.2",
        "@types/body-parser": "^1.19.6",
        "@types/compression": "^1.8.1",
        "@types/country-flag-icons": "^1.2.2",
        "@types/express": "^5.0.6",
        "@types/htmlescape": "^1.1.3",
        "@types/lodash-es": "^4.17.12",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.10.6",
        "@types/object-hash": "^3.0.6",
        "@types/qs": "^6.14.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "concurrently": "^9.2.1",
        "oxfmt": "^0.23.0",
        "oxlint": "^1.38.0",
        "oxlint-tsgolint": "^0.11.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3",
        "vite": "^7.3.1"
    }
}
