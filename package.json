{
    "name": "ui-server",
    "version": "1.0.0",
    "description": "ui-server for the app",
    "main": "./dist/server/index.js",
    "keywords": [
        "express",
        "jwt",
        "react",
        "ssr"
    ],
    "author": "Ninad Tungare",
    "email": "ninad.tungare@gmail.com",
    "license": "MIT",
    "packageManager": "pnpm@10.24.0",
    "scripts": {
        "check": "tsc --noEmit --extendedDiagnostics",
        "clean": "rm -rf dist",
        "build": "pnpm run build:node && pnpm run build:bundles",
        "build:node": "NODE_ENV=production tsc",
        "build:bundles": "pnpm run build:bundle:client --mode=production && pnpm run build:bundle:server --mode=production",
        "build:bundle:client": "vite build --config ./vite.client.config.ts",
        "build:bundle:server": "vite build --config ./vite.server.config.ts",
        "start": "node ./dist/server/index.js",
        "start:dev": "concurrently --prefixColors auto pnpm:start:dev:node pnpm:start:dev:bundle:client pnpm:start:dev:bundle:server",
        "start:dev:node": "pnpm run wait-on && NODE_ENV=development ts-node-dev --inspect=9220 -- ./src/server/index.ts",
        "start:dev:bundles": "concurrently --prefixColors auto pnpm:start:dev:bundle:client pnpm:start:dev:bundle:server",
        "start:dev:bundle:client": "pnpm run build:bundle:client --minify=false --watch --mode=development",
        "start:dev:bundle:server": "pnpm run build:bundle:server --minify=false --watch --mode=development",
        "wait-on": "node ./wait-on.mjs",
        "lint": "oxlint .",
        "format": "oxfmt"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.90.12",
        "@tanstack/react-query-devtools": "^5.91.1",
        "axios": "^1.13.2",
        "body-parser": "^2.2.1",
        "classnames": "^2.5.1",
        "compression": "^1.8.1",
        "express": "^5.2.1",
        "htmlescape": "^1.1.1",
        "lodash-es": "^4.17.21",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "tailwindcss": "^4.1.17",
        "utf-8-validate": "^6.0.5",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@tailwindcss/vite": "^4.1.17",
        "@tanstack/eslint-plugin-query": "^5.91.2",
        "@types/body-parser": "^1.19.6",
        "@types/compression": "^1.8.1",
        "@types/express": "^5.0.6",
        "@types/htmlescape": "^1.1.3",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.1",
        "globals": "^16.5.0",
        "oxfmt": "^0.16.0",
        "oxlint": "^1.32.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.1",
        "vite": "^7.2.7",
        "wait-on": "^9.0.3"
    }
}